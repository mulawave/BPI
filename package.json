    "youtube:scheduler": "npx tsx server/jobs/youtubeScheduler.ts",
    "db:export": "npx tsx scripts/exportDatabaseToSeed.ts",
    "db:backup": "npx tsx scripts/exportDatabaseToSeed.ts",
    "db:restore": "npx tsx prisma/seedFromBackup.ts",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force && npm run db:seed",
    "backup:git": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/gitAutoBackup.ps1",
    "backup:schedule": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/scheduleGitAutoBackup.ps1"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },

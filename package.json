    "smoke:admin-activity": "npx tsx scripts/smokeAdminActivity.ts",
    "smoke:admin-activity-export": "npx tsx scripts/smokeAdminActivityExport.ts",
    "smoke:user-ui": "set BASE_URL=http://localhost:3000&& npx playwright test tests/user-.*\\.spec\\.ts$ tests/smoke-user-flows.spec.ts --workers=1",
    "smoke:admin-ui": "set BASE_URL=http://localhost:3000&& npx playwright test tests/admin-.*\\.spec\\.ts$ --workers=1",
